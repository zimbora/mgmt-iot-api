{
  "name": "mgmt-iot-api",
  "version": "1.0.12",
  "description": "api to access mgmt-iot server",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=github-actions --coverageReporters=text --coverageReporters=lcov --coverageReporters=json-summary"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "index.js",
      "config/**/*.js",
      "!config/keys.js",
      "!**/*.test.js",
      "!**/__tests__/**"
    ]
  },
  "author": "lucas",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.3.6",
    "form-data": "^4.0.4",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "jest": "^29.5.0"
  }
}